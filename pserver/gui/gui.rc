    POPUP "帮助(&H)"
    BEGIN
        MENUITEM "在线帮助(&H)\tCtrl+H",            ID_ONLINE_HELP
        MENUITEM SEPARATOR
        MENUITEM "检查更新(&C)\tCtrl+C",            ID_CHK_UPDATE
        MENUITEM SEPARATOR
        MENUITEM "关于Digi-e打印服务 (&A)\tCtrl+A",   ID_APP_ABOUT
    END
END

